            "key": true,
            "type": "cds.UUID"
          },
          "displayId": {
            "type": "cds.String"
          },
          "Customer": {
            "type": "cds.UUID"
          },
          "account": {
            "type": "cds.Association",
            "target": "ProjectOrderService.Accounts",
